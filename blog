          for (var f = 0; f < e.feed.category.length; f++) {
            var category = e.feed.category[f].term;
            $('.widget_categories ul').append('<li class="cat-item"><a href="/search/label/'+category+'?max-results=12">'+category+'</a></li>');
          }
        }
        if (e.feed.entry) {
          for (var f = 0; f < e.feed.entry.length; f++) {
            var entry = e.feed.entry[f],
              entry_title = entry.title.$t
            for (var a = 0; a < entry.link.length; a++) {
              if (entry.link[a].rel == 'alternate') {
                var entry_alternate = entry.link[a].href
                break
              }
            }
            if ('media$thumbnail' in entry) {
              var entry_thumb = entry.media$thumbnail.url.replace('s72-c', 's300').replace('s72-', '').replace('/default','/maxresdefault')
            } else {
              var st = entry.content.$t,
                at = st.indexOf("<img"),
                bt = st.indexOf('src="', at),
                ct = st.indexOf('"', bt + 5),
                dt = st.substr(bt + 5, ct - bt - 5)
              if (at != -1 && bt != -1 && ct != -1 && dt != "") entry_thumb = dt
              else entry_thumb = 'https://4.bp.blogspot.com/-00O66C-eBQs/W0IcokXSnOI/AAAAAAAAL_k/g4KtDm7SkQsoe7_G0vZ_C_nU0Gf_-kyVQCLcBGAs/s1600/safe_image.png'
            }
            $('#recent').append('<li class="eltdf-bl-item eltdf-item-space clearfix"><div class="eltdf-bli-inner"><div class="eltdf-post-image"> <a itemprop="url" href="'+entry_alternate+'"> <img width="150" height="150" src="'+entry_thumb+'" class="attachment-thumbnail size-thumbnail wp-post-image" alt="'+entry_title+'" loading="lazy"> </a></div><div class="eltdf-bli-content"><h3 class="entry-title eltdf-post-title" style="font-size:18px;font-family:Roboto"> <a itemprop="url" href="'+entry_alternate+'"> '+entry_title+'</a></h3></div></div></li>');
          }
        }
      }
    })
    $.ajax({
      type: 'GET',
      url: '/feeds/posts/default/-/Blog',
      data: {
        'max-results': 500,
        'alt': 'json'
      },
      dataType: 'jsonp',
      success: function(e) {
        if (e.feed.entry) {
          for (var f = 0; f < 3; f++) {
			let randomIndex = Math.floor(Math.random() * e.feed.entry.length);
            var entry = e.feed.entry[randomIndex],
              entry_title = entry.title.$t
            for (var a = 0; a < entry.link.length; a++) {
              if (entry.link[a].rel == 'alternate') {
                var entry_alternate = entry.link[a].href
                break
              }
            }
            if ('media$thumbnail' in entry) {
              var entry_thumb = entry.media$thumbnail.url.replace('s72-c', 'w300-h300-p-k-no-nu').replace('s72-', '').replace('/default','/maxresdefault')
            } else {
              var st = entry.content.$t,
                at = st.indexOf("<img"),
                bt = st.indexOf('src="', at),
                ct = st.indexOf('"', bt + 5),
                dt = st.substr(bt + 5, ct - bt - 5)
              if (at != -1 && bt != -1 && ct != -1 && dt != "") entry_thumb = dt
              else entry_thumb = 'https://4.bp.blogspot.com/-00O66C-eBQs/W0IcokXSnOI/AAAAAAAAL_k/g4KtDm7SkQsoe7_G0vZ_C_nU0Gf_-kyVQCLcBGAs/s1600/safe_image.png'
            }
            $('#related').append('<div class="eltdf-related-post"><div class="eltdf-related-post-inner"><div class="eltdf-related-post-image"> <a itemprop="url" href="'+entry_alternate+'"> <img width="2560" height="1707" src="'+entry_thumb+'" class="attachment-full size-full wp-post-image" alt="'+entry_title+'" loading="lazy"/> </a></div><h4 itemprop="name" class="entry-title eltdf-post-title"><a itemprop="url" href="'+entry_alternate+'">'+entry_title+'</a></h4></div></div>');
          }
        }
      }
    })
//]]></script>
</b:if>
  
