<script>//<![CDATA[
    $.ajax({
      type: 'GET',
      url: '/feeds/posts/default/-/Video',
      data: {
        'max-results': 8,
        'alt': 'json'
      },
      dataType: 'jsonp',
      success: function(e) {
        if (e.feed.entry) {
          for (var f = 0; f < 3; f++) {
			let randomIndex = Math.floor(Math.random() * e.feed.entry.length);
            var entry = e.feed.entry[randomIndex],
              entry_title = entry.title.$t
            for (var a = 0; a < entry.link.length; a++) {
              if (entry.link[a].rel == 'alternate') {
                var entry_alternate = entry.link[a].href
                break
              }
            }
            if ('media$thumbnail' in entry) {
              var entry_thumb = entry.media$thumbnail.url.replace('s72-c', 'w300-h300-p-k-no-nu').replace('s72-', '').replace('/default','/maxresdefault')
            } else {
              var st = entry.content.$t,
                at = st.indexOf("<img"),
                bt = st.indexOf('src="', at),
                ct = st.indexOf('"', bt + 5),
                dt = st.substr(bt + 5, ct - bt - 5)
              if (at != -1 && bt != -1 && ct != -1 && dt != "") entry_thumb = dt
              else entry_thumb = 'https://4.bp.blogspot.com/-00O66C-eBQs/W0IcokXSnOI/AAAAAAAAL_k/g4KtDm7SkQsoe7_G0vZ_C_nU0Gf_-kyVQCLcBGAs/s1600/safe_image.png'
            }
            $('#related2').append('<div class="eltdf-related-post" style="width:25%"><div class="eltdf-related-post-inner"><div class="eltdf-related-post-image"> <a href="'+entry_alternate+'"> <img width="2560" height="1707" src="'+entry_thumb+'" class="attachment-full size-full wp-post-image" alt="'+entry_title+'" loading="lazy"/> </a></div><h4 itemprop="name" class="entry-title eltdf-post-title" style="font-size:16px;font-family:Arial;margin:10px 0 0"><a itemprop="url" href="'+entry_alternate+'">'+entry_title+'</a></h4></div></div>');
          }
        }
      }
    })
//]]></script>
